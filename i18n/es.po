"                                        <strong t-attf-style=\"color: #{grade.grade &gt;= 5.0 and '#27ae60' or '#c0392b'}\">\n"
"                                            <t t-esc=\"'%.2f' % grade.grade\"/>\n"
"                                        </strong>\n"
"                                    </td>\n"
"                                </tr>\n"
"                            </t>\n"
"                        </tbody>\n"
"                    </table>\n"
"\n"
"                    <!-- Final Message -->\n"
"                    <p style=\"margin: 15px 0;\">\n"
"                        For more details, please review the attached PDF file.\n"
"                    </p>\n"
"\n"
"                    <!-- Signature -->\n"
"                    <div style=\"margin-top: 20px; padding-top: 15px; border-top: 1px solid #ecf0f1;\">\n"
"                        <p style=\"margin: 0;\">Best regards,</p>\n"
"                        <p style=\"margin: 5px 0; color: #2c3e50;\">\n"
"                            <strong><t t-esc=\"object.university_id.name\"/></strong><br/>\n"
"                            <span style=\"color: #7f8c8d;\">Academic Administration</span>\n"
"                        </p>\n"
"                    </div>\n"
"\n"
"                    <!-- Footer -->\n"
"                    <div style=\"margin-top: 30px; padding-top: 15px; border-top: 1px solid #ecf0f1;                                  text-align: center; font-size: 12px; color: #95a5a6;\">\n"
"                        This is an automated message. Please do not reply directly to this email.\n"
"                    </div>\n"
"                </div>\n"
"            "

